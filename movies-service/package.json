{
  "name": "express-ts-openapi3",
  "version": "1.0.0",
  "repository": "express-ts-openapi3",
  "description": "express-ts-openapi3",
  "main": "dist/index.js",
  "dependencies": {
    "async-retry": "^1.2.3",
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.3.6",
    "bluebird": "^3.5.4",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "2.8.4",
    "debug": "^3.1.0",
    "express": "4.16.4",
    "helmet": "^3.16.0",
    "inversify": "^5.0.1",
    "js-yaml": "^3.13.1",
    "jsonapi-serializer": "^3.6.4",
    "multer": "^1.4.1",
    "node-fetch": "^2.3.0",
    "node-uuid": "^1.4.8",
    "oas-tools": "^2.1.5",
    "opossum": "^1.11.0",
    "perfy": "^1.1.2",
    "prom-client": "11.0.0",
    "reflect-metadata": "^0.1.13",
    "roi": "^0.18.0",
    "serverless-mysql": "^1.3.0",
    "source-map-support": "^0.5.12",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/chai-http": "3.0.3",
    "@types/cls-hooked": "^4.3.0",
    "@types/debug": "0.0.30",
    "@types/express": "4.16.1",
    "@types/mocha": "2.2.43",
    "@types/node": "12.0.0",
    "@types/node-uuid": "0.0.28",
    "@types/opossum": "^1.10.0",
    "@types/swagger-tools": "0.10.4",
    "@types/winston": "^2.3.7",
    "chai": "4.2.0",
    "chai-http": "^4.2.1",
    "codecov": "^3.3.0",
    "cpx": "1.5.0",
    "db-migrate": "^0.11.5",
    "db-migrate-mysql": "^1.1.10",
    "kill-port": "^1.3.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.11",
    "nyc": "^14.1.0",
    "serverless": "^1.40.0",
    "shelljs": "^0.7.8",
    "sinon": "^4.1.5",
    "supertest": "3.0.0",
    "ts-mocha": "1.2.0",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.5.3"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:copy": "cpx \"src/**/*.yaml\" dist/src",
    "start": "DEBUG=app:* node dist/index.js",
    "build:live": "npm run clean && npm run lint && tsc -p tsconfig.json && npm run build:copy && cp package* ./dist && npm install --production --prefix ./dist",
    "build:dev": "nodemon --exec ts-node -- ./src/index.ts",
    "dev": "DEBUG=app:*,fetch-circuit-breaker TS_NODE_CACHE=false npm run build:dev",
    "lint": "tslint -p tsconfig.json",
    "updateSwagger": "node scripts/updateSwagger.js",
    "updateAll": "node scripts/updateCode.js",
    "test": "DEBUG=app:* TS_NODE_CACHE=false tsc -p tsconfig.json --noEmit --skipLibCheck && nyc mocha --timeout 10000 --require ts-node/register 'test/**/*.ts' --exit",
    "testOne": "TS_NODE_CACHE=false tsc -p tsconfig.json --noEmit --skipLibCheck && nyc mocha --timeout 10000 --require ts-node/register ",
    "integration-test": "nyc mocha integration-test/index.js",
    "jmeter": "rm -f integration-test/log.jtl && rm -rf integration-test/report && jmeter -n -t integration-test/movie-service-api.jmeter.test.jmx -l integration-test/log.jtl -e -o integration-test/report",
    "codecov": "nyc report --reporter=lcovonly && codecov",
    "deploy": "npm run build:live && serverless deploy",
    "chrome-debug": "node --debug-brk --inspect --require ts-node/register src/index.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "text-summary",
      "html"
    ],
    "sourceMap": true
  },
  "engines": {
    "node": ">6.11.0"
  },
  "author": "Daisuke Hirata",
  "license": "ISC"
}
