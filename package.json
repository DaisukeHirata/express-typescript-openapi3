{
  "name": "express-ts-openapi3",
  "version": "1.0.0",
  "repository": "express-ts-openapi3",
  "description": "express-ts-openapi3",
  "main": "dist/index.js",
  "dependencies": {
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.3.6",
    "bluebird": "^3.5.4",
    "cls-hooked": "^4.2.2",
    "cors": "2.8.4",
    "debug": "^3.1.0",
    "express": "4.16.2",
    "helmet": "^3.16.0",
    "js-yaml": "^3.13.1",
    "jsonapi-serializer": "^3.6.4",
    "multer": "^1.4.1",
    "node-uuid": "^1.4.8",
    "oas-tools": "^2.1.5",
    "perfy": "^1.1.2",
    "prom-client": "11.0.0",
    "serverless-mysql": "^1.2.1",
    "source-map-support": "^0.5.12",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/chai-http": "3.0.3",
    "@types/cls-hooked": "^4.3.0",
    "@types/debug": "0.0.30",
    "@types/express": "4.0.39",
    "@types/mocha": "2.2.43",
    "@types/node": "8.0.46",
    "@types/node-uuid": "0.0.28",
    "@types/swagger-tools": "0.10.4",
    "@types/winston": "^2.3.7",
    "chai": "4.1.2",
    "chai-http": "^4.2.1",
    "cpx": "1.5.0",
    "db-migrate": "^0.11.5",
    "db-migrate-mysql": "^1.1.10",
    "inversify": "^5.0.1",
    "kill-port": "^1.3.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.11",
    "reflect-metadata": "^0.1.13",
    "serverless": "^1.40.0",
    "shelljs": "^0.7.8",
    "sinon": "^4.1.5",
    "supertest": "3.0.0",
    "ts-mocha": "1.2.0",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.5.3"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:copy": "cpx \"src/**/*.yaml\" dist/",
    "start": "DEBUG=fes:* node dist/index.js",
    "build:live": "npm run clean && npm run lint && tsc -p tsconfig.json && npm run build:copy && cp package* ./dist && npm install --production --prefix ./dist",
    "test": "DEBUG=fes:* TS_NODE_CACHE=false ./node_modules/.bin/mocha --timeout 10000 --compilers ts:ts-node/register ./test/* --exit",
    "build:dev": "nodemon --exec ts-node -- ./src/index.ts",
    "dev": "DEBUG=fes:*,app:* TS_NODE_CACHE=false npm run build:dev",
    "lint": "tslint -p tsconfig.json",
    "updateSwagger": "node scripts/updateSwagger.js",
    "updateAll": "node scripts/updateCode.js",
    "testOne": "TS_NODE_CACHE=false ./node_modules/.bin/mocha --timeout 10000 --compilers ts:ts-node/register ",
    "deploy": "npm run build:live && serverless deploy"
  },
  "engines": {
    "node": ">6.11.0"
  },
